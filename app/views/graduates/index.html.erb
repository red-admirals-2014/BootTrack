<div data-comp="topbar">
  <span data-comp="logo">BootTrack</span>
</div>
<div data-comp="banner">
  <h1 data-comp="tag-line">Description text goes here.
  </h1>
  <div data-comp="search-container">
    <form>
      <select data-comp="campus" name="location" id="location">
        <option selected="selected" value="default">Campus...</option>
        <option value="chicago">Chicago</option>
        <option value="new_york">New York</option>
        <option value="san_francisco">San Francisco</option>
      </select>
      <select data-comp="year" name="year">
        <option selected="selected" value="default">Year...</option>
        <option value="2014">2014</option>
        <option value="2013">2013</option>
        <option value="2012">2012</option>
      </select>
      <input data-comp="button" type="submit" value="Search">
    </form>
  </div>
</div>
<div data-comp='footer'>
  <span>Footer goes here.</span>
</div>
